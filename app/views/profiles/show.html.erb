<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @profile.name %>
</p>

<p>
  <b>Description:</b>
  <%= @profile.description %>
</p>

<p>
  <b>Public:</b>
  <%= @profile.public %>
</p>

<ul>
<% if !@profile.profile_activities.blank? %>
  <% for activity in @profile.profile_activities %>
    <li>activity.name</li>
  <% end %>
<% else %>
 <li>No activities</li>
<% end %>
</ul>


<%= link_to 'Edit', edit_profile_path(@profile) %> |
<%= link_to 'Back', profiles_path %>
